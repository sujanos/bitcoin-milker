{
  "name": "vincent-dca",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo for vincent DCA backend + frontend",
  "homepage": "https://github.com/LIT-protocol/vincent-dca",
  "engines": {
    "node": "^20.11.1",
    "pnpm": "10.7.0"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r lint",
    "clean": "pnpm exec rm -Rf node_modules dist && pnpm --parallel -r run clean",
    "test": "pnpm -r run test",
    "dev": "pnpm build && pnpm -r run dev",
    "prepare": "husky",
    "start": "pnpm -r run start",
    "link-local-vincent-sdk-dev": "pnpm dotenvx run -- ./linkVincentSdkLocaldev.sh"
  },
  "packageManager": "pnpm@10.7.0",
  "keywords": [],
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.26.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.11.30",
    "@types/verror": "^1.10.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-plus": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.29.0",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@lit-protocol/vincent-sdk": "^0.0.2",
    "consola": "^3.2.3",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "overrides": {}
  }
}
